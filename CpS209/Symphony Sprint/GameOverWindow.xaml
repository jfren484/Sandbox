<Window x:Class="Symphony_Sprint.GameOverWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Symphony_Sprint"
        mc:Ignorable="d"
        Title="GameOverWindow" Height="350" Width="450">
    
    <Canvas Background="Black">
        <StackPanel>
            <TextBlock Height="100" Width="395" Padding="25" Margin="25,70,25,15" Text="Game Over" Foreground="White" FontSize="50" TextAlignment="Center" FontFamily="Agency FB"/>
            <Button Content="OK" Height="30" Width="170" Background="Gold" Click="btnOK_Click"/>
        </StackPanel>
        <!--<Image Source="Graphics/help-1.png.png" Height="90" Margin="0,0,0,0"/>-->
        
    </Canvas>

</Window>
